####################### MAY NEED EDITING ########################
#Compiler options
CC=g++
#CPU="-mcpu=i686"
CPU=
#OPT: if set to dbg > debug version
#OPT: if set to opt > optimised version
OPT=opt
#PIC if wanted shared dynamic libraries
PIC=-fpic
#libray paths
MQTDIR=$(QTDIR)
UNIXLIB =  -L$(MQTDIR)/lib -lqt-mt -L/usr/X11R6/lib -lglut -lGL -lGLU  
#include paths
IFLAGS =  -I$(MQTDIR)/include 
IFLAGS += -I$(PIGALEPATH)/incl
#awk 
AWK=awk
####################### END EDITING #########################
PIGALEPATH=..
NVERSION=BETA
# P = name of output program (a "s" is prepended for static version)
ifeq ($(OPT),opt)
P=Pigale
else
P=Pigale_dbg
endif

# L = name of libray
L=pigaleQt
MMOC=$(MQTDIR)/bin/moc
PIGALELIB =  -L$(PIGALEPATH)/lib    -l$(L)$(OPT) -ltgraph$(OPT)

#GENERAL OPTIONS
.SUFFIXES:.cpp .o .obj .opt .dbg
#OPTIMIZED OPTIONS
CFLAGSopt = -O3  $(PIC) $(CPU)
#CFLAGSopt +=  -Wall -Wpointer-arith  -pipe
CFLAGSopt +=  -Wall -Wpointer-arith -Werror  -pipe
CFLAGSopt +=  -fomit-frame-pointer 
CFLAGSopt +=  -DQT -DVERSION_$(NVERSION) 
CFLAGSopt +=  -DQT_THREAD_SUPPORT 

#DEBUGGING OPTIONS
CFLAGSdbg  =  -g -Wall $(PIC) $(CPU)
CFLAGSdbg +=  -Wpointer-arith -Werror -pipe
CFLAGSdbg +=  -DTDEBUG
CFLAGSdbg +=  -DQT -DVERSION_$(NVERSION)
CFLAGSdbg +=  -DQT_THREAD_SUPPORT 

CFLAGS  = $(CFLAGS$(OPT))
VPATH   = ./.obj
DLIB    = $(PIGALEPATH)/lib/lib$L$(OPT).so
FILEALL := $(wildcard *.cpp)
FILES   := $(filter-out client.cpp moc_client.cpp, $(FILEALL))
INCLALL := $(wildcard *.h)
INCL    := $(filter-out client.h, $(INCLALL))
MOC_CPP := $(patsubst %.h,moc_%.cpp, $(INCL))
FILES_WO_MOC := $(filter-out $(MOC_CPP), $(FILES))
FILES_W_MOC  := $(FILES_WO_MOC)
FILES_W_MOC  += $(MOC_CPP)
OBJECTS       =  $(patsubst %.cpp,%.$(OPT), $(FILES_W_MOC))
OBJ           =  $(patsubst %.cpp,$(VPATH)/%.$(OPT), $(FILES_W_MOC))
ALLOBJ        = $(wildcard $(VPATH)/*.$(OPT))
ALLOBJ        += $(wildcard $(PIGALEPATH)/tgraph/.obj/*.$(OPT))

SALLOBJ    = $(wildcard $(VPATH)/*.$(OPT))
SLIBOBJ := $(filter-out $(VPATH)/main.$(OPT), $(SALLOBJ))

all : md $(P) 

$(PIGALEPATH)/incl/QT/Action.h:$(PIGALEPATH)/incl/QT/Action_def.h Action.awk
	$(AWK) -f Action.awk $(PIGALEPATH)/incl/QT/Action_def.h > $(PIGALEPATH)/incl/QT/Action.h

moc_%.cpp:%.h
	$(MMOC) $< -o $@

.cpp.$(OPT): $*.cpp
	$(CC) -c $(CFLAGS) $(IFLAGS) -o  $(VPATH)/$*.$(OPT)  $*.cpp

$(DLIB):$(OBJECTS) 
	@rm -f $(PIGALEPATH)/lib/lib$L$(OPT).so
	@$(CC) -shared  -o $(PIGALEPATH)/lib/lib$L$(OPT).so  $(OBJ)

$(P) : $(DLIB) $(PIGALEPATH)/lib/libtgraph$(OPT).so 
#	$(CC)  $(CFLAGS) -o $(P)  $(PIGALELIB)  $(UNIXLIB) 
	g++ -o $(P) $(VPATH)/main.$(OPT) $(PIGALEPATH)/lib/libpigaleQt$(OPT).so $(PIGALEPATH)/lib/libtgraph$(OPT).so  $(UNIXLIB)
	
clean:
	@rm -f *~ *bak *.flc  moc_*.cpp  $(OBJ) $(DLIB)
	
md: depend
	@if ! test -d .obj ; then mkdir .obj ; fi
test:
	@echo SALLOBJ=$(SALLOBJ)

#static:$(DLIB) $(PIGALEPATH)/lib/libtgraph$(OPT).so 
#	g++  $(CFLAGS) -o s$(P) $(ALLOBJ)  $(UNIXLIB) 

static:$(SALLOBJ) 
	@ar cru $(PIGALEPATH)/lib/lib$L$(OPT).la  $(SLIBOBJ)
	@ranlib $(PIGALEPATH)/lib/lib$L$(OPT).la
	g++ -o s$(P) $(VPATH)/main.$(OPT) $(PIGALEPATH)/lib/libpigaleQt$(OPT).la $(PIGALEPATH)/lib/libtgraph$(OPT).la  $(UNIXLIB)


moc_cpp:    $(MOC_CPP)

depend:
	@makedepend -o.$(OPT)  -fgnumakefile -I$(PIGALEPATH)/incl -I$(PIGALEPATH) -Y   $(FILES_WO_MOC)  2> /dev/null

nodepend:
	@makedepend -o.$(OPT)  -fgnumakefile -Y    2> /dev/null
.PHONY: md all
# DO NOT DELETE

